<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Hospital Patient Squence -->
        <record id="hospital_patient_sequence" model="ir.sequence">
            <field name="name">Hospital Patient Sequence</field>
            <field name="code">hospital.patient</field>
            <field name="prefix">HP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Appointments Sequence -->
        <record id="hospital_appointments_sequence" model="ir.sequence">
            <field name="name">Hospital Appointments Sequence</field>
            <field name="code">hospital.appointments</field>
            <field name="prefix">HA</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
